    </collision>
  </link>
  <joint name="body_bodyright_joint" type="fixed">
    <origin xyz="0.0 -0.125 0" rpy="0 ${-90 * deg_to_rad} 0"/>
    <child link="bodyright_link"/>
    <parent link="body_link"/>
    
