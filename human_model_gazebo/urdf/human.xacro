      <mass value="5"/>
      <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>

    </inertial>
    <visual>
      <geometry>
        <cylinder radius="0.05" length="${human_back_length}"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <!-- origin is relative -->
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="${human_back_length}"/>
