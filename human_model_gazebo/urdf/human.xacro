<?xml version="1.0"?>
<robot name="human" xmlns:xacro="http://www.ros.org/wiki/xacro" >
<!--
Robot model taken from http://wiki.ros.org/pr2_mechanism/Tutorials/SImple%20URDF-Controller%20Example
-->
  <xacro:include filename="$(find human_model_gazebo)/urdf/wheel.xacro"/>

  <xacro:property name="deg_to_rad" value="0.01745329251994329577"/>

  <!-- Constants for robot dimensions -->
  <xacro:property name="human_back_length" value="0.5" /> 
  <xacro:property name="length" value="0.4" /> 
  <xacro:property name="width" value="0.4" /> 
  <xacro:property name="wheel_radius" value="0.11" /> <!-- Link 1 -->
  <xacro:property name="thickness" value="0.086" /> <!-- Link 2 -->
  <xacro:property name="axel_offset" value="0.05" /> <!-- Space btw top of beam and the each joint -->
  <xacro:property name="steer_offset" value="0.02" /> <!-- Link 1 -->
  <xacro:property name="steer_height" value="0.4" />
  <xacro:property name="board_length" value="0.8" />
  <xacro:property name="board_width" value="0.3" /> 

  <material name="white">
    <color rgba="0.45 0.45 0.45 1"/>
  </material>

  <material name="wood">
    <color rgba="0.54509 0.2705 0.0745 1"/>
  </material>

  <material name="black">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="green">
    <color rgba="0.95 0.95 0.95 1"/>
  </material>

  <material name="pink">
    <color rgba="1 0.4 1 1"/>
  </material>

   <material name="orange">
    <color rgba="0 1 1 1"/>
  </material>

   <material name="blue">
    <color rgba="0 0 1 0.5"/>
  </material>

  <!-- Links: inertial,visual,collision -->
  <link name="base_human_link">
    <!-- <inertial>
      <origin xyz="0 0 0.2" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
    </inertial> -->
    <visual>
      <geometry>
        <box size="${length} ${width} 0.04"/>
      </geometry>
      <material name="wood"/>
    </visual>
    <collision>
      <!-- origin is relative -->
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="${length} ${width} 0.04"/>
      </geometry>
    </collision>
  </link>

  <link name="base_footprint">
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.00000001"/>
      </geometry>
      <material name="white"/>
    </collision>
  </link>
  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 ${wheel_radius}" rpy="0 0 0"/>
    <parent link="base_human_link"/>
    <child link="base_footprint"/>
  </joint>

  <!-- Links: inertial,visual,collision -->
  <link name="back_link">
    <inertial>
      <!-- origin is relative -->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
    </inertial>
    <visual>
      <geometry>
        <box size="${human_back_length} ${width} 0.04"/>
      </geometry>
      <material name="wood"/>
    </visual>
    <collision>
      <!-- origin is relative -->
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="${length} ${width} 0.04"/>
      </geometry>
    </collision>
  </link>
  <joint name="base_back_joint" type="fixed">
      <origin xyz="${-length/2.0 + 0.02} 0 ${length/2.0}" rpy="0 ${-90 * deg_to_rad} 0"/>
    <child link="back_link"/>
    <parent link="base_human_link"/>
  </joint>

 <!-- -->


  <!-- Wheels -->
  <front_wheel name="front_right" parent="base_human" radius="${wheel_radius}" thickness="${thickness}" 
      length="${length}" width="${width}" axel_offset="${axel_offset}" right_left="1" steer_height="${steer_height}">
  </front_wheel>
  <front_wheel name="front_left" parent="base_human" radius="${wheel_radius}" thickness="${thickness}" 
      length="${length}" width="${width}" axel_offset="${axel_offset}" right_left="-1" steer_height="${steer_height}">
  </front_wheel>
  <rear_wheel name="rear_right" parent="base_human" radius="${wheel_radius}" thickness="${thickness}" 
      length="${length}" width="${width}" axel_offset="${axel_offset}" right_left="1" steer_height="${steer_height}">
  </rear_wheel>
  <rear_wheel name="rear_left" parent="base_human" radius="${wheel_radius}" thickness="${thickness}" 
      length="${length}" width="${width}" axel_offset="${axel_offset}" right_left="-1" steer_height="${steer_height}">
  </rear_wheel>

  <!-- Links: inertial,visual,collision -->
  <link name="body_link">
    <inertial>
      <!-- origin is relative -->
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia ixx="5" ixy="0" ixz="0" iyy="5" iyz="0" izz="5"/>
